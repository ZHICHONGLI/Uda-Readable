{
  "env": {
      "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "ecmaFeatures": {
      "jsx": true,
      "es6": true
  },
  "rules": {
      "comma-dangle": 1,
      "indent": [2, 2, {"SwitchCase": 1}],
      "react/prop-types": 0
  },
  "plugins": [
    "babel",
    "react"
  ]
}